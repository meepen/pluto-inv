"VertexlitGeneric"
{
	"$basetexture" "models/player/anon/body_d"
	"$bumpmap" "models/player/anon/body_n"
	"$halflambert" 1
	"$phong" "1"
	"$phongtint" "[0.9 0.8 0.7]"
	"$phongboost" "12.0"
	"$phongexponent" "20"
	"$phongfresnelranges" "[0.4 0.2 1]"
	
	"$rimlight" "1"
	"$rimlightexponent" "1"
	"$rimlightboost" "0.5"
	
	"$selfillum" "1"
	
	"$blendtintbybasealpha" "0" 
 	"$blendtintcoloroverbase" "1"

	Proxies
	{
		PlayerColor
		{
			resultVar	$selfillumtint
			resultVar   $color2
			default		0.86 0.85 0.83
		}
		Clamp
		{
			min 10.00
			max 20.0
			srcVar1 "$selfillumtint"
			resultVar "$selfillumtint"
		}
		Clamp
		{
			min 0.00
			max 0.40
			srcVar1 "$color2"
			resultvar "$color2"
		}
	}	
}	

